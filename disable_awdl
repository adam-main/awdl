mkdir -p ~/Library/LaunchAgents
curl -o ~/Library/LaunchAgents/launched.disable-awdlsm.plist https://launched.zerowidth.com/plists/de50ca6f-8283-451c-a0ba-85d08db1c281.xml
sudo cp ~/Library/LaunchAgents/launched.disable-awdlsm.plist /Library/LaunchDaemons
sudo launchctl load -w /Library/LaunchDaemons/launched.unhook-awdl
